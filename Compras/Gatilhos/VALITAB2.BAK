User Function VALITAB2
	a_Area   = GetArea()
	c_prod   := M->C7_PRODUTO
	c_preco  := 0.00
		
	DBSELECTAREA("AIB")      
	dbgotop()
	DO while .t.
	   if AIB->AIB_CODPRO = C_PROD
	      c_preco = AIB->AIB_PRCCOM 
          Alert("Produto com preco tabelado, preço minimo sera o da tabela cadastro!!!")
	      exiT
	   endif
	   dbskip()
	   if eof()
	      exit
	   endif
	enddo        
	
	 

	RestArea(a_Area)
Return c_preco