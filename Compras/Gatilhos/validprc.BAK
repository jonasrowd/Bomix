User Function validprc
	a_Area   := GetArea()
	c_prod   := M->C7_PRODUTO
	c_preco  := M->C7_preco
		
	SELECTAREA("AIB")      
	dbgotop
	DO while .t.
	   if AIB->AIB_CODPRO = C_PROD
	      c_preco = AIB->AIB_PRCCOM 
          Alert("Produto com preco tabelado, preço minimo sera o da tabela cadastro!!!")
	      exist
	   endif
	   dbskip()
	   if eof()
	      exit
	   endif
	enddo      

	RestArea(a_Area)
Return c_preco