Static Function f_ChkUsr
	Local c_CodGrps := ''						//Códigos dos grupos cadastrados no parametro FS_CODGRP
	Local a_CodGrps := {}						//Array com os códigos dos grupos cadastrados no parametro FS_CODGRP
	Local a_GrpUsr  := UsrRetGrp(__CUSERID)     //Array contendo os códigos dos grupos que o usuário pertence
	Local l_Ret     := .F.
	
	If GETMV("FS_CODGRP", .T., )
		c_CodGrps := AllTrim(GETMV("FS_CODGRP"))
	Else
		ShowHelpDlg(SM0->M0_NOME, {"O parâmetro FS_CODGRP não está cadastrado na SX6."},5,;
	  	{"Contacte o administrador do sistema e solicite a criação deste parâmetro."},5)
	  	Return()
	Endif

	If Empty(c_CodGrps)
		l_Ret := .F.
	Else
		a_CodGrps := StrTokArr(c_CodGrps, ";")
	  	For i := 1 To Len(a_CodGrps)
	  		For k:= 1 To Len(a_GrpUsr)
		   		If a_CodGrps[i] == a_GrpUsr[k]
		    		l_Ret := .T.
		    		Exit
		   		Endif
			Next k
			
			If l_Ret
				Exit
			Endif
	  	Next i
	Endif

	If !l_Ret
		ShowHelpDlg(SM0->M0_NOME, {"Você não possui permissão de acesso a esta rotina."},5,;
	  	{"Contacte o administrador do sistema e solicite acesso a esta rotina."},5)
	Endif
Return l_Ret

