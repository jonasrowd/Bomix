Static Function xPrintRel()  

	Local nX 		:= 0
	Local nQtdPag 	:= 0
 	Local c_NumOp	:= SH6->H6_OP
 	Local c_DescPro	:= ""
 	Local c_Lote    := ""
 	Local n_QtdPE	:= SC2->C2_FSQTDPE   
    Local c_Seq		:= "01"
    
	Private oPrint
	Private cAlias 	    := getNextAlias() //cria um alias temporário
	Private oFont6		:= TFont():New('Arial',,6,,.F.,,,,.F.,.F.)
	Private oFont6n		:= TFont():New('Arial',,6,,.T.,,,,.F.,.F.)
	Private oFont8		:= TFont():New('Arial',,8,,.F.,,,,.F.,.F.)
	Private oFont8n		:= TFont():New('Arial',,8,,.T.,,,,.F.,.F.)
	Private oFont10		:= TFont():New('Arial',,10,,.F.,,,,.F.,.F.)
	Private oFont10n	:= TFont():New('Arial',,10,,.t.,,,,.F.,.F.)
	Private nLin		:= 0 

/*    
    dbSelectArea("SZ3")
    dbSetOrder(1)
    dbSeek(SC2->C2_FILIAL+SC2->(C2_NUM + C2_ITEM + C2_SEQUEN))
    While (!EoF()) .AND. SZ3->Z3_FILIAL == SC2->C2_FILIAL .AND.SZ3->Z3_NUMOP == SC2->(C2_NUM + C2_ITEM + C2_SEQUEN)
    	//Se possui apontamento
		If SZ3->Z3_APONT .AND. Val(c_Seq) < Val(SZ3->Z3_SEQ)   					    				
		    c_Seq := SZ3->Z3_SEQ
	    EndIf
	    SZ3->(DbSkip())
    EndDo        
*/

 	If MsgBox("Deseja efetuar a impressão das etiquetas e geração do número do lote?",SM0->M0_NOME,"YESNO")
		dbSelectArea("SB1")
		dbGoTop()
		dbSetOrder(1)
		dbSeek(xFilial("SB1")+SC2->C2_PRODUTO)

		c_DescPro := SB1->B1_DESC

		If SB1->B1_RASTRO == 'L'				//Verifica se o produto possui rastreabilidade
			c_Qry := f_Qry()                    //Chama a função para selecionar o lote de maior sequencial da OP

			TCQUERY c_Qry NEW ALIAS QRY
			dbSelectArea("QRY")
			dbGoTop()
			If QRY->(!Eof()) .And. !Empty(QRY->H6_LOTECTL)                    //Se existir o maior lote, altera o valor da variável c_Seq
				c_Seq := StrZero(Val(SubStr(QRY->H6_LOTECTL,9,2)) + 1, 2, 0)
			Endif

			c_Lote := c_NumOp + c_Seq	//O lote retornado será o número da OP mais o sequencial

			dbCloseArea("QRY")
		Endif

		oPrint := TMSPrinter():New(OemToAnsi('Etiquetas de Ordens de Produção'))
		oPrint:SetPortrait()  

		nQtdPag := mv_par01

		For nX := 1 to nQtdPag
			oPrint:StartPage()  

 			oPrint:Box( 020,045,325,585 )           

			oPrint:Box( 020,605,325,1145 )	 			 

			nLin  := 0030
			oPrint:Say(nLin,0050,MemoLine(c_DescPro,29,1),oFont10n,,,,0)
			oPrint:Say(nLin,0610,MemoLine(c_DescPro,29,1),oFont10n,,,,0)		

			nLin += 0030

 			oPrint:Say(nLin,0050,MemoLine(c_DescPro,30,2),oFont10n,,,,0)
			oPrint:Say(nLin,0610,MemoLine(c_DescPro,30,2),oFont10n,,,,0)

 			nLin += 0070
									 			        			
 			oPrint:Say(nLin,0050,"Contém " + cValToChar(n_QtdPE) + " Und.",oFont8,,,,0)
 			oPrint:Say(nLin,0610,"Contém " + cValToChar(n_QtdPE) + " Und.",oFont8,,,,0)

 			oPrint:Say(nLin,0280,"Lote: " +c_Lote,oFont8,,,,0)
 			oPrint:Say(nLin,0840,"Lote: " +c_Lote,oFont8,,,,0)

 			nLin += 0070

 			oPrint:Say(nLin,0050,"Data Fabricação: "+dtoc(dDataBase),oFont8,,,,0)
			oPrint:Say(nLin,0610,"Data Fabricação: "+dtoc(dDataBase),oFont8,,,,0)

			nLin += 0070

			oPrint:Say(nLin,0050,"BOMIX Indústria de Embalagens Ltda.",oFont8,,,,0)
			oPrint:Say(nLin,0610,"BOMIX Indústria de Embalagens Ltda.",oFont8,,,,0) 			

			oPrint:EndPage()
		Next nX

//		oPrint:Preview()
 		oPrint:Print({1},nQtdPag)
		oPrint:end()
	EndIf
Return

